{
  "default": true,
  "MD001": true,
  "MD003": { "style": "atx" },
  "MD004": { "style": "dash" },
  "MD007": { "indent": 2 },
  "MD013": {
    "line_length": 120,
    "code_blocks": false,
    "tables": false,
    "headings": false
  },
  "MD024": { "siblings_only": true },
  "MD025": true,
  "MD026": { "punctuation": ".,;:!" },
  "MD029": { "style": "ordered" },
  "MD033": {
    "allowed_elements": [
      "details",
      "summary",
      "br",
      "img",
      "a",
      "sub",
      "sup",
      "kbd",
      "div",
      "span",
      "table",
      "tr",
      "td",
      "th"
    ]
  },
  "MD034": false,
  "MD041": false,
  "MD028": false,
  "no-hard-tabs": true,
  "whitespace": true,
  "commands-show-output": false,
  "no-trailing-spaces": true,
  "no-multiple-blanks": { "maximum": 2 }
}
